@model ECommerce.Models.Dashboard

@{
    ViewData["Title"] = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div>
    <h4>Dashboard</h4>
    <hr />
    <dl class="row">

        <div class="card-body">
            
            
            <p><strong>Total Products : </strong> @Model.TotalProducts.ToString()</p>
            <p><strong>Total Orders : </strong> @Model.TotalOrders</p>
           
        </div>
    </dl>
</div>
